{
  "name": "math-speed-backend",
  "version": "1.0.0",
  "description": "Express TypeScript backend with Mongoose",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "dev": "bun run --watch index.ts",
    "build": "bun build index.ts --outdir dist --target node",
    "start": "bun run dist/index.js",
    "openapi": "openapi typegen --backend public/openapi.yaml > src/types/openapi.d.ts",
    "type-check": "tsc --noEmit",
    "seed": "bun run src/scripts/seed.ts"
  },
  "keywords": [
    "express",
    "typescript",
    "mongoose",
    "bun"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@types/jsonwebtoken": "^9.0.10",
    "cors": "^2.8.5",
    "dotenv": "^17.2.1",
    "express": "^5.1.0",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.17.1",
    "ms": "^2.1.3",
    "openapi-backend": "^5.14.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/node": "^24.3.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2"
  }
}
